{
	"include": [
		"wwwroot/js/*.ts"
	],
	"compileOnSave": true,
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"module": "amd",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noEmitOnError": false,
		"outDir": "./wwwroot/js/dist",
		"removeComments": false,
		"sourceMap": true,
		"noImplicitUseStrict": true,
		"skipDefaultLibCheck": true,
		"target": "es5",
		"typeRoots": [
			"./node_modules/@types"
		]
	},
	"exclude": [
		"node_modules",
		"wwwroot/lib/rxjs/**",
		"**/*.spec.ts"
	]
}
