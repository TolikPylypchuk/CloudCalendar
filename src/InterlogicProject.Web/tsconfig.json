{
	"include": [
		"wwwroot/js/*.ts"
	],
	"compileOnSave": true,
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"typeRoots": [
			"./wwwroot/lib/moment/moment",
			"./node_modules/@types"
		]
	},
	"exclude": [
		"node_modules",
		"wwwroot/lib",
		"**/*.spec.ts"
	]
}
