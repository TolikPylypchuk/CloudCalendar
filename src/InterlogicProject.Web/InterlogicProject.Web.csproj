<Project Sdk="Microsoft.NET.Sdk.Web" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>netcoreapp1.1</TargetFramework>
    <NoWarn>1701;1702;1705;CS1591;IDE1006</NoWarn>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PreserveCompilationContext>true</PreserveCompilationContext>
    <AssemblyName>InterlogicProject.Web</AssemblyName>
    <OutputType>Exe</OutputType>
    <PackageTargetFallback Condition=" '$(TargetFramework)' == 'netcoreapp1.0' ">$(PackageTargetFallback);dotnet5.6;portable-net45+win8</PackageTargetFallback>
  </PropertyGroup>
  <ItemGroup>
    <Content Include="wwwroot\**\*;web.config">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\InterlogicProject.DAL\InterlogicProject.DAL.csproj" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.NETCore.App">
      <Version>1.0.1</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Diagnostics">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Mvc">
      <Version>1.0.1</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Server.IISIntegration">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Server.Kestrel">
      <Version>1.0.1</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.StaticFiles">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer">
      <Version>1.0.1</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools">
      <Version>1.0.0-msbuild1-final</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Extensions.Configuration.Json">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="Microsoft.Extensions.Logging.Console">
      <Version>1.0.0</Version>
    </PackageReference>
    <PackageReference Include="AutoMapper">
      <Version>5.1.1</Version>
    </PackageReference>
    <PackageReference Include="AutoMapper.Extensions.Microsoft.DependencyInjection">
      <Version>1.1.2</Version>
    </PackageReference>
    <PackageReference Include="Swashbuckle.SwaggerGen">
      <Version>6.0.0-beta901</Version>
    </PackageReference>
    <PackageReference Include="Swashbuckle.SwaggerUi">
      <Version>6.0.0-beta901</Version>
    </PackageReference>
  </ItemGroup>
  <ItemGroup>
    <DotNetCliToolReference Include="Microsoft.EntityFrameworkCore.Tools.DotNet">
      <Version>1.0.0-msbuild1-final</Version>
    </DotNetCliToolReference>
  </ItemGroup>
  <ItemGroup>
    <Content Update=".bowerrc">
      <DependentUpon>bower.json</DependentUpon>
    </Content>
    <Content Update="bundleconfig.json.bindings">
      <DependentUpon>bundleconfig.json</DependentUpon>
    </Content>
    <Content Update="project.lock.json">
      <DependentUpon>project.json</DependentUpon>
    </Content>
    <Content Update="runtimeconfig.template.json">
      <DependentUpon>project.json</DependentUpon>
    </Content>
    <Content Update="wwwroot\css\dist\style.min.css">
      <DependentUpon>style.css</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\callendarLecturer.js.map">
      <DependentUpon>callendarLecturer.js</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\callendarLecturer.min.js">
      <DependentUpon>callendarLecturer.js</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\callendarStudent.js.map">
      <DependentUpon>callendarStudent.js</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\callendarStudent.min.js">
      <DependentUpon>callendarStudent.js</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\models.js.map">
      <DependentUpon>models.js</DependentUpon>
    </Content>
    <Content Update="wwwroot\js\dist\requireConfig.min.js">
      <DependentUpon>requireConfig.js</DependentUpon>
    </Content>
  </ItemGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>$(DefineConstants);RELEASE</DefineConstants>
  </PropertyGroup>
  <Target Name="PostpublishScript" AfterTargets="Publish" Condition=" '$(IsCrossTargetingBuild)' != 'true' " />
</Project>